<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
	<class name="com.rk.entity.InsurancePolicy" table="Insurance_Policy">
	
	<id name="policy_id" length="10" type="long"/>
	
	<property name="policy_name" length="20" type="string"/>
	
	
	<property name="policy_type" length="20" type="string"/>
	
	<property name="company" length="20" type="string"/>
	
	<property name="tenure" length="20" type="string"/>
	</class>
	
	
	
	</hibernate-mapping>