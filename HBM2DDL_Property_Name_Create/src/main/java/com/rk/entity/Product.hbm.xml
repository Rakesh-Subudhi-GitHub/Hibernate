<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
	
	<class name="com.rk.entity.Product" table="PRODUCT_TEST" >
	
	<id name="pid"  column="PID" length="10" type="long" >
	<generator class ="increment"/>
	</id>
	
	<property name="name" column="name" length="20" type="string" not-null="true"/>
	<property name="addres" column="addres" length="15" type="string" not-null="true"/>
	<property name="price" column="price"  type="long" length="10" precision="2" not-null="true"/>
	
	</class>
	
	
	</hibernate-mapping>